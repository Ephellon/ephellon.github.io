<cross-domain-policy>
    <site-control
        permitted-cross-domain-policies="all"
        />

    <allow-access-from
        domain="*"
        secure="false"
        to-ports="50000"
        />

    <allow-http-request-headers-from
        domain="*"
        headers="*"
        secure="false"/>
</cross-domain-policy>
